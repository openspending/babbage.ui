{
  "name": "babbage.ui",
  "description": "Angular components to use the Cubes Slicer API provided by Babbage.",
  "homepage": "https://github.com/openspending/babbage.ui",
  "license": "MIT",
  "version": "1.1.2",
  "main": "lib/index.js",
  "scripts": {
    "build:lib": "gulp",
    "build:dist": "for i in $(ls -d ./src/bindings/*/); do i=${i##./src/bindings/}; i=${i%%/}; BUILD_TARGET=$i webpack --config webpack.config.development.js; done",
    "build:dist:min": "for i in $(ls -d ./src/bindings/*/); do i=${i##./src/bindings/}; i=${i%%/}; BUILD_TARGET=$i webpack --config webpack.config.production.js; done",
    "build": "npm run build:lib && npm run build:dist && npm run build:dist:min",
    "prepublish": "npm run build",
    "review": "jscs lib test",
    "fix": "jscs lib test --fix",
    "test": "mocha"
  },
  "files": [
    "lib",
    "dist",
    "src"
  ],
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.15.0",
    "bluebird": "^3.4.1",
    "bubbletree": "git+https://github.com/okfn/bubbletree.git",
    "c3": "git+https://github.com/masayuki0812/c3.git",
    "css-loader": "^0.25.0",
    "d3": "^3.5.16",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.5.1",
    "pivottable": "git+https://github.com/nicolaskruchten/pivottable.git",
    "raphael": "^2.1.4",
    "vue-hot-reload-api": "^2.0.6"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.5.0",
    "bootstrap": "3.3.4",
    "chai": "^3.4.1",
    "concat-with-sourcemaps": "^1.0.4",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-include-file": "git+https://github.com/tweedegolf/gulp-include-file.git",
    "gulp-less": "^3.0.5",
    "gulp-minify-css": "^1.2.4",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "html-minifier": "^0.7.2",
    "jscs": "^2.8.0",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "mocha-jsdom": "^1.0.0",
    "nock": "^7.2.2",
    "raw-loader": "^0.5.1",
    "through2": "^2.0.1",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^8.5.3",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openspending/babbage.ui.git"
